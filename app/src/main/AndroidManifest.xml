<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ashton.tictactoe">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Game_Over"
            android:theme="@style/AppTheme.PopUp"
            ></activity>
        <activity android:name=".Game"></activity>
        <activity android:name=".offer_draw"
            android:theme="@style/AppTheme.PopUpTheme"
            ></activity>
        <activity android:name=".recordedGames"></activity>
        <activity android:name=".game_playback"></activity>
        <activity android:name=".recordGamesSortPopUp"
            android:theme="@style/AppTheme.PopUpTheme"></activity>
    </application>

</manifest>